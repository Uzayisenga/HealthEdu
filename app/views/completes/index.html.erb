<p id="notice"><%= notice %></p>

<h1>Completes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Favorite</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @completes.each do |complete| %>
      <tr>
        <td><%= complete.user_id %></td>
        <td><%= complete.favorite_id %></td>
        <td><%= complete.status %></td>
        <td><%= link_to 'Show', complete %></td>
        <td><%= link_to 'Edit', edit_complete_path(complete) %></td>
        <td><%= link_to 'Destroy', complete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complete', new_complete_path %>
