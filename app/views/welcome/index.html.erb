                        </div>
                    </div>
                </div>
                <!--End of row-->
                <% Course.all.each do |course| %>
                                <div class="row">
                                <div class="col-md-4">
                                <div class="card-group">
                                <div class="card">
                                  <img src="..." class="card-img-top" alt="...">
                                  <div class="card-body">
                                    <h5 class="card-image"><%= course.image %></h5><br>
                                    <p class="card-text"><%= truncate course.course, length:50 %></p><br>
                                    <p class="card-text"><%= course.time %></p><br>
                                    <p class="card-text"><%= course.price %></p><br>
                                    <p class="card-text"><%= course.user_id %></p><br>
                                  </div>
                                  <div class="card-footer">
                                    <small class="text-muted"><%= link_to 'Read More', course %></small>
                                  </div>
                                </div><br><br><br>
                                
                                </div>
                                </div>
